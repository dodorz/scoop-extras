            "hash": "fc1a1e230a97e950f16e701e081bcddf6e0c659eefede8826210d885f0b4a174"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/AK1SgOvEL1Z__ik41P_VukA_92.0.4497.0/92.0.4497.0_chrome_installer.exe#/dl.7z",
            "hash": "ef1006cee6448e1430778ce32dff32fc6b925fe3dba2056703f5fa81910ddc41"
        }
    },
    "installer": {
        "script": "Expand-7zipArchive \"$dir\\chrome.7z\" -ExtractDir 'Chrome-bin' -Removal"
